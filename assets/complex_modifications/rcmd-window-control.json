{
    "title": "RCmd",
    "rules": [
        {
            "description": "RCmd+J/K -> Cmd+Tab/Cmd+Shift+Tab",
            "manipulators": [
                {
                    "type": "basic",
                    "from": { "key_code": "j", "modifiers": { "mandatory": [ "right_command" ] } },
                    "to": [ { "key_code": "tab", "modifiers": [ "command" ] } ]
                },
                {
                    "type": "basic",
                    "from": { "key_code": "k", "modifiers": { "mandatory": [ "right_command" ] } },
                    "to": [ { "key_code": "tab", "modifiers": [ "command", "shift" ] } ]
                }
            ]
        },
        {
            "description": "RCmd+L -> Next App. Window(command+`)",
            "manipulators": [
                {
                    "type": "basic",
                    "from": { "key_code": "l", "modifiers": { "mandatory": [ "right_command" ] } },
                    "to": [ { "key_code": "grave_accent_and_tilde", "modifiers": [ "command" ] } ]
                }
            ]
        },
        {
            "description": "RCmd+M -> Cmd+H",
            "manipulators": [
                {
                    "type": "basic",
                    "from": { "key_code": "m", "modifiers": { "mandatory": [ "right_command" ] } },
                    "to": [ { "key_code": "h", "modifiers": [ "command" ] } ]
                }
            ]
        }
    ]
}
